<launch>
    <include file="$(find pamaral_models)/launch/right_hand_keypoints_detection.launch">
        <arg name="input_image_topic" value="front_camera/color/image_raw"/>
    </include>

    <node name="dataset_processing" pkg="pamaral_models" type="mp_failure_metrics.py" output="screen">
        <param name="input_folder" value="$(find data)/bag_files/mp_failure_metrics"/>
    </node>

    <node name="rviz" pkg="rviz" type="rviz"
          args="--display-config $(find pamaral_models)/config/rviz.rviz" required="false"/>
</launch>