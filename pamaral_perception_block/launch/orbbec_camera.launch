<launch>
    <!--Color Camera-->
    <remap from="/usb_cam/image_raw" to="/camera/color/image_raw"/>

    <include file="$(find pamaral_perception_block)/launch/usb_cam.launch">
        <arg name="image_view" value="false" />
    </include>

    <!--Depth and IR Camera-->
    <include file="$(find astra_camera)/launch/astra_pro.launch">
        <arg name="enable_point_cloud" value="false"/>
        <arg name="enable_color" value="false"/>
        <arg name="depth_fps" default="30"/>
        <arg name="enable_ir" default="false"/>
        <arg name="publish_tf" default="false"/>
        <arg name="use_uvc_camera" value="false"/>
    </include>
</launch>